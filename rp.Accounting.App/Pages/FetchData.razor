@page "/fetchdata"

<h1>Weather forecast</h1>

<p>This component demonstrates fetching data from a service.</p>

<table class="table">
    <thead>
        <tr>
            <th>Date</th>
            <th>Temp. (C)</th>
            <th>Temp. (F)</th>
            <th>Summary</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < 5; i++)
        {
        <tr>
            <td>@DateTime.Now.ToShortDateString()</td>
            <td>23 C</td>
            <td>40 F</td>
            <td>Warm</td>
        </tr>
        }
    </tbody>
</table>

